{
  "expo": {
    "name": "AviTrack",
    "slug": "avitrack",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "avitrack",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#34C759"
      },
      "permissions": [
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.USE_EXACT_ALARM"
      ],
      "package": "com.luis000923.avitrack"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      "expo-splash-screen"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0640c74b-4e0e-46c9-9d7e-e2eaaed752c9"
      }
    },
    "owner": "luis000923s-organization"
  }
}
